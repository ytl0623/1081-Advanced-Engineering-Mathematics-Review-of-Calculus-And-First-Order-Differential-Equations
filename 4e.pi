import numpy as np
import matplotlib.pyplot as plt
from sympy import*

def F1(x) :
  y = np.tan(x) - x - 1
  return y

x = np.linspace( 0, 4*np.pi, 100 )
y = F1(x)

plt.plot( x, y, '-' )
plt.xlabel( 'x' )
plt.ylabel( 'y' )
plt.title( "f(x) = tam(x) - x - 1" )
plt.text( 10, -70, 'Copyright@ytl' )

plt.show()