import numpy as np
import matplotlib.pyplot as plt
from sympy import*

def F1(x) :
  y = np.sin( x**2 / 2 )
  return y

x = np.linspace( 0, 2*np.pi, 100 )
y = F1(x)

plt.plot( x, y, '-' )
plt.xlabel( 'x' )
plt.ylabel( 'y' )
plt.title( "f(x) = sin( x**2 / 2 )" )
plt.text( 5, 1, 'Copyright@ytl' )

plt.show()